<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:id="@+id/millennial_main_layout"
    tools:context=".MillennialActivity" >

    <Button
        android:id="@+id/callMMVideo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:onClick="mmVideo"
        android:text="@string/mmVideo" />

    <Button
        android:id="@+id/callMMImage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/callMMVideo"
        android:layout_alignBottom="@+id/callMMVideo"
        android:layout_centerHorizontal="true"
        android:onClick="mmImage"
        android:text="@string/mmImage" />

    <Button
        android:id="@+id/callMMInterstitial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/callMMImage"
        android:layout_alignBottom="@+id/callMMImage"
        android:layout_alignParentRight="true"
        android:onClick="mmInterstitial"
        android:text="@string/mmInterstitial" />

</RelativeLayout>
